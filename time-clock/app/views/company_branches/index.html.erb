<p id="notice"><%= notice %></p>

<h1>Company Branches</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_branches.each do |company_branch| %>
      <tr>
        <td><%= company_branch.name %></td>
        <td><%= company_branch.address %></td>
        <td><%= link_to 'Show', company_branch %></td>
        <td><%= link_to 'Edit', edit_company_branch_path(company_branch) %></td>
        <td><%= link_to 'Destroy', company_branch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company Branch', new_company_branch_path %>
